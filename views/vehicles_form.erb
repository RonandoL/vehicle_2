<div class="jumbotron">
  <h1>Vehicles Form</h1>
  <h2 class="well">add new vehicle below</h2>
</div>


<form action="/vehicles" method="post">

  <div class="form-group">
    <label for="make">Make:</label>
    <input id="make" name="make" class="form-control" type="text" placeholder="add make of car">
  </div>

  <div class="form-group">
    <label for="model">Model:</label>
    <input id="model" name="model" class="form-control" type="text" placeholder="add model of car">
  </div>

  <div class="form-group">
    <label for="year">Year:</label>
    <input id="year" name="year" class="form-control" type="text" placeholder="add year of car">
  </div>

  <button type="submit" class="btn btn-info">Submit your car</button>
</form>
